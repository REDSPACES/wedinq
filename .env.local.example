# Firebase Client SDK (Next.js クライアント側で使用)
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=wedinq-local
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# Firebase Emulator を使用するかどうか（開発環境では true）
NEXT_PUBLIC_USE_FIREBASE_EMULATOR=true

# Firebase Admin SDK (Server Components/Actions で使用)
# 本番環境でのみ必要。開発環境ではエミュレータを使用するため不要
# FIREBASE_PROJECT_ID=
# FIREBASE_SERVICE_ACCOUNT_KEY=
